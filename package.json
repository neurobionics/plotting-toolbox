{
	"name": "plotting-toolbox",
	"displayName": "Plotting Toolbox",
	"description": "A VSCode extension that plots values from a CSV file.",
	"publisher": "opensourceleg",
	"author": {
		"name": "Senthur Ayyappan",
		"email": "senthura@umich.edu"
	},
	"license": "LGPL-2.1-only",
	"repository": {
		"type": "git",
		"url": "https://github.com/imsenthur/plotting-toolbox.git"
	},
	"version": "0.0.2",
	"engines": {
		"vscode": "^1.91.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [],
	"main": "./dist/extension.js",
	"contributes": {
		"menus": {
			"explorer/context": [
				{
					"when": "resourceExtname == .csv",
					"command": "plottingToolbox.plot",
					"group": "navigation"
				}
			]
		},
		"commands": [
			{
				"command": "plottingToolbox.plot",
				"title": "Plotting Toolbox: Plot"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "webpack",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"compile-tests": "tsc -p . --outDir out",
		"watch-tests": "tsc -p . -w --outDir out",
		"pretest": "npm run compile-tests && npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "vscode-test"
	},
	"devDependencies": {
		"@types/mocha": "^10.0.7",
		"@types/node": "20.x",
		"@types/vscode": "^1.91.0",
		"@typescript-eslint/eslint-plugin": "^7.14.1",
		"@typescript-eslint/parser": "^7.11.0",
		"@vscode/test-cli": "^0.0.9",
		"@vscode/test-electron": "^2.4.0",
		"eslint": "^8.57.0",
		"ts-loader": "^9.5.1",
		"typescript": "^5.4.5",
		"webpack": "^5.92.1",
		"webpack-cli": "^5.1.4"
	},
	"dependencies": {
		"csv-parse": "^5.5.6",
		"csv-parser": "^3.0.0"
	}
}
